<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.muliba.fancyfilepickerlibrary">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name=".ui.FilePickerActivity"
            android:label="@string/title_activity_file_picker"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.FileClassificationPickerActivity"
            android:label="@string/title_activity_file_picker"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.ImageClipActivity"></activity>
    </application>

</manifest>